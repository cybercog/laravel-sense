{
    "name": "cybercog/laravel-sense",
    "description": "Laravel Application Audit.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cog",
        "cybercog",
        "laravel",
        "eloquent",
        "database",
        "db",
        "query",
        "audit",
        "profiling",
        "debug",
        "sense",
        "smell"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://komarev.com/sources/laravel-sense",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-sense/issues",
        "wiki": "https://github.com/cybercog/laravel-sense/wiki",
        "source": "https://github.com/cybercog/laravel-sense",
        "docs": "https://github.com/cybercog/laravel-sense/wiki"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Laravel\\Sense\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Tests\\Laravel\\Sense\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Laravel\\Sense\\Providers\\SenseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
