{
    "name": "cybercog/laravel-sense",
    "description": "Laravel Application Audit.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cog",
        "cybercog",
        "laravel",
        "eloquent",
        "database",
        "db",
        "query",
        "audit",
        "profiling",
        "debug",
        "sense",
        "smell"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "a.komarev@cybercog.su",
            "homepage": "https://ell.folio.su",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "mockery/mockery": "^1.0",
        "orchestra/database": "~3.5.0|~3.6.0|~3.7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Contracts\\Laravel\\Sense\\": "contracts/",
            "Cog\\Laravel\\Sense\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Tests\\Laravel\\Sense\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Laravel\\Sense\\Providers\\SenseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
